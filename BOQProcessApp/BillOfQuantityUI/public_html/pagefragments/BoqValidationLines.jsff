            <af:commandButton text="No" id="cb16"
                              actionListener="#{backingBeanScope.BoqUploadLinesBean.CostCodeConfirmationNo}"/>
          </af:group>
        </f:facet>
      </af:dialog>
    </af:popup>
  </af:panelGroupLayout>
