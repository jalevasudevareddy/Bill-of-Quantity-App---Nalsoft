    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Variable
    Name="p_hdr_id"
    Kind="where"
    Type="java.math.BigDecimal"/>
  <Variable
    Name="p_line_seq_id"
    Kind="where"
    Type="java.math.BigDecimal"/>
  <SQLQuery>
    <![CDATA[SELECT l.boq_level,
  l.page_item,
  l.item,
  l.bill_description,
  l.bill_quantity,
  l.final_quantity,
  l.uom,
  l.candy_net_rate,
  l.selling_rate,
  l.candy_net_final_amount,
  l.selling_amount,
  l.trade,
  l.price_code,
  cc.cost_code,
  cc.cost_code_description,
  vr.resource_code,
  vr.resource_description,
  vr.uom resc_uom,
  vr.net_use_unit,
  vr.final_rate,
  ue.table_code,
  ue.reference_id
FROM xxboq_validation_lines_t l,
  xxboq_validation_cost_code_t cc,
  xxboq_validation_resource_t vr,
  xx_upload_errors_t ue
WHERE l.validation_header_id   = cc.validation_header_id
AND cc.validation_header_id    = vr.validation_header_id
AND l.validation_line_id       = cc.validation_line_id
AND cc.validation_cost_code_id = vr.validation_cost_code_id
AND l.validation_header_id     =:p_hdr_id
AND (NVL(l.line_sequence,-99)  =:p_line_seq_id
OR NVL(cc.line_sequence, -99)  =:p_line_seq_id
OR NVL(vr.line_sequence, -99)  =:p_line_seq_id)
AND ue.upload_id               = l.validation_header_id
AND ue.reference_id            =
  CASE
    WHEN ue.table_code='BOQ_LINES'
    THEN l.validation_line_id
    WHEN ue.table_code='BOQ_COST_CODES'
    THEN cc.validation_cost_code_id
    WHEN ue.table_code='BOQ_RESOURCE_CODES'
    THEN vr.validation_resource_id
  END
AND ue.line_number =:p_line_seq_id]]>
  </SQLQuery>
