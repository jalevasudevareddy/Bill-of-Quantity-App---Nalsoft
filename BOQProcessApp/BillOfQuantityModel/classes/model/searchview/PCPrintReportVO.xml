  <SQLQuery>
    <![CDATA[SELECT
     type,
     pc_header_id,
     bu_id,
     bu_name,
     contract_id,
     doc_type,
     contract_name,
     contract_number,
     start_date,
     end_date,
     party_name,
     contract_sum,
     pc_document_num,
     pc_date,
     period,
     payment_due_date,
     wd_main_contract_value,
     mat_at_site,
     wd_var_contract_value,
     wd_cla_contract_value,
     POST_COMP_TOC_WORKS,
     nvl(wd_main_contract_value,0)+nvl(wd_var_contract_value,0)+nvl(wd_cla_contract_value,0)
+nvl(POST_COMP_TOC_WORKS,0) total_works,
     adv_paid,
     nvl(wd_main_contract_value,0)+nvl(wd_var_contract_value,0)+nvl(wd_cla_contract_value,0)
+nvl(mat_at_site,0)+nvl(POST_COMP_TOC_WORKS,0)+nvl(adv_paid,0) total_valuations,
     gross_amount,
     round((adv_recovered/contract_sum)*100,2) advance_recovery_prct,
     adv_recovered,
     round((retention/contract_sum)*100,2) retention_prct,
     retention,
     RETENTION_RELEASE,
     oth_ded,
     nvl(retention,0)+nvl(adv_recovered,0)+nvl(oth_ded,0)-nvl(RETENTION_RELEASE,0) total_deductions,
     nvl(wd_main_contract_value,0)+nvl(wd_var_contract_value,0)+nvl(wd_cla_contract_value,0)
+nvl(mat_at_site,0)+nvl(POST_COMP_TOC_WORKS,0)+nvl(adv_paid,0)
-(nvl(retention,0)+nvl(adv_recovered,0)+nvl(oth_ded,0)-nvl(RETENTION_RELEASE,0))net_valuation,
     cumm_total,
     prev_total,
     prev_pc_document_num,
     currency_code,
     CONTRACTOR,
     engineer,
     VALUATION_DATE,
     INVOICE_DOCUMENT_NUM,
     description,
     case when end_date is not null then end_date - start_date end time_for_completion,
     xxfin_amount_into_words_pkg.words( nvl(wd_main_contract_value,0)+nvl(wd_var_contract_value,0)+nvl(wd_cla_contract_value,0)
+nvl(mat_at_site,0)+nvl(POST_COMP_TOC_WORKS,0)+nvl(adv_paid,0)
-(nvl(retention,0)+nvl(adv_recovered,0)+nvl(oth_ded,0)-nvl(RETENTION_RELEASE,0))-prev_total,currency_code) amount_in_words
 FROM
     xxboq_pc_print_rv pp
 WHERE
     pp.pc_header_id =:p_pc_header_id
     AND pp.type =:p_report_type]]>
  </SQLQuery>
