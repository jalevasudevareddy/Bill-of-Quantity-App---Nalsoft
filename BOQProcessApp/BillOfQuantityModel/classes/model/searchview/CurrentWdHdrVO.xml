  <SQLQuery>
    <![CDATA[SELECT  WD_HEADER_ID
,BUSINESS_UNIT_ID
,PROJECT_ID
,PERIOD_OF_MEASURE
,WD_DOCUMENT_DATE
,WD_DOCUMENT_NUM
,WD_STATUS
,WD_APPROVED_DATE
,WD_APPROVED_BY
,CREATION_DATE
,CREATED_BY
,LAST_UPDATED_DATE
,LAST_UPDATED_BY
,VERSION
,MASTER_WD_HEADER_ID
,PARENT_WD_HEADER_ID
,MASTER_BOQ_HEADER_ID
,BOQ_HEADER_ID
,CURRENT_FLAG
,FILE_NAME,
valuation_date,
(select nvl(sum(nvl(selling_amount,0)),0) from xxboq_bill_of_qty_lines_t boql where boql.price_code is not null and boq_header_id = wd.BOQ_HEADER_ID) total_Revenue_Amount,
(select sum(nvl(wl.wd_cummulative_qty,0)*wl.current_boq_rate) from xxboq_work_done_lines_t wl where wl.wd_header_id = wd.wd_header_id) current_wd_amount,
(select sum(nvl(wl.wd_cummulative_qty,0)*wl.current_boq_rate) from xxboq_work_done_lines_t wl where wl.wd_header_id = wd.wd_header_id and (select line_type from xxboq_bill_of_qty_lines_t bl where bl.boq_line_id = wl.boq_line_id)='M') current_main_contract_wd_amount FROM
  XXBOQ_WORK_DONE_DETAILS_T WD
WHERE
  WD.WD_HEADER_ID = (
    SELECT
      MAX(WD1.WD_HEADER_ID)
    FROM
      XXBOQ_WORK_DONE_DETAILS_T wd1
    WHERE
      wd1.contract_ID =:p_contract_id
      AND wd1.WD_STATUS = 'A'
      AND TO_DATE(wd1.PERIOD_OF_MEASURE,'Mon-yyyy') <= to_date(:p_period,'Mon-yyyy')
  )]]>
  </SQLQuery>
