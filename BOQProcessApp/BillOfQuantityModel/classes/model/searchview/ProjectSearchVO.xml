  <SQLQuery>
    <![CDATA[select pj.PROJECT_ID,pj.PROJECT_NUMBER, pj.PROJECT_NAME,BU_ID ,pj.START_DATE,pj.END_DATE ,
(select party_id from XXPJ_PROJECT_CUSTOMERS_T pc where pc.project_id=pj.PROJECT_ID) customer_id,
(select party_name from XXPJ_PROJECT_CUSTOMERS_T pc where pc.project_id=pj.PROJECT_ID) customer_name,
(select ADVANCE_PRCT from XXPJ_PROJECT_CUSTOMERS_T pc where pc.project_id=pj.PROJECT_ID) ADVANCE_PRCT,
(select ADVANCE_RECOVERY_PRCT from XXPJ_PROJECT_CUSTOMERS_T pc where pc.project_id=pj.PROJECT_ID) ADVANCE_RECOVERY_PRCT,
(select RETENTION_PRCT from XXPJ_PROJECT_CUSTOMERS_T pc where pc.project_id=pj.PROJECT_ID) RETENTION_PRCT,
(select PAYMENT_TERM_NAME from XXPJ_PROJECT_CUSTOMERS_T pc where pc.project_id=pj.PROJECT_ID)PAYMENT_TERM_NAME,
(select BOQ_HEADER_ID from XXBOQ_BILL_OF_QTY_HDR_T bh where bh.project_id = pj.project_id and bh.status='AC') boq_header_id
from xxpj_projects_t pj where (pj.PROJECT_STATUS_CODE IN ('APPROVED','ACTIVE') OR exists
        (SELECT PSC.PROJECT_STATUS_NAME FROM pjf_proj_status_controls PSC WHERE PSC.PROJECT_STATUS_CODE = PJ.PROJECT_STATUS_CODE AND PSC.PROJECT_STATUS_NAME='Only BOQ'))
AND NVL (pj.TEMPLATE_FLAG, 'N') = 'N'
AND pj.BU_ID                   = :p_bu_id 
and XXPJ_ACCESS_FUN(pj.PROJECT_ID,:p_user_id)='Y'
and upper(nvl(pj.PROJECT_STATUS_CODE,'N')) not in ('CLOSED','CANCELLED','ON HOLD')
AND TRUNC(SYSDATE) BETWEEN pj.START_DATE AND NVL(END_DATE,SYSDATE)
AND upper(NVL(pj.TEMPLATE_FLAG,'N')) = 'N']]>
  </SQLQuery>
