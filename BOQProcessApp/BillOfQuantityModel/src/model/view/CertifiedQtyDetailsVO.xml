       XxboqCertifiedDetailsEO.MASTER_CERT_HEADER_ID, 
       XxboqCertifiedDetailsEO.MASTER_INVOICE_HEADER_ID, 
       XxboqCertifiedDetailsEO.PARENT_CERT_HEADER_ID, 
       XxboqCertifiedDetailsEO.INVOICE_HEADER_ID, 
       (select  case when   (select max(CERTIFIED_HEADER_ID) max_hdr_id from         XXBOQ_CERTIFIED_DETAILS_T where CONTRACT_ID= XxboqCertifiedDetailsEO.CONTRACT_ID  and BUSINESS_UNIT_ID= XxboqCertifiedDetailsEO.BUSINESS_UNIT_ID  and CERTIFIED_STATUS !='CL') = XxboqCertifiedDetailsEO.CERTIFIED_HEADER_ID  then   'Y' else 'N' end from dual) AS VIEW_ATTR, 
       (select INVOICE_DOCUMENT_NUM||' '||VERSION  from XXBOQ_INVOICE_DETAILS_T where INVOICE_HEADER_ID=XxboqCertifiedDetailsEO.INVOICE_HEADER_ID) AS VIEW_ATTR, 
       XxboqCertifiedDetailsEO.CURRENT_FLAG, 
       XxboqCertifiedDetailsEO.BOQ_HEADER_ID, 
       (select case when count(1) >0 then 'Y' else 'N' end isPCExtsFlg from XXBOQ_PC_HDR_T where CERTIFIED_HEADER_ID = XxboqCertifiedDetailsEO.CERTIFIED_HEADER_ID and PC_STATUS!='CL' ) AS VIEW_ATTR, 
       XxboqCertifiedDetailsEO.CURRENT_FLAG AS CURRENT_FLAG1, 
       ( SELECT   nvl(SUM(nvl(lines.SELLING_AMOUNT,0) ),0)     FROM   XXBOQ_BILL_OF_QTY_HDR_T hdr,   XXBOQ_BILL_OF_QTY_LINES_T lines     WHERE   hdr.BOQ_HEADER_ID = lines.BOQ_HEADER_ID   AND hdr.contract_id = XxboqCertifiedDetailsEO.contract_id   AND hdr.BUSINESS_UNIT_ID = XxboqCertifiedDetailsEO.BUSINESS_UNIT_ID   AND hdr.current_flag = 'Y'   AND lines.item IS NOT NULL and lines.bill_page_item is not null) AS VIEW_ATTR, 
       (SELECT nvl( XxfiCurrenciesEO.PRECISION,2)    FROM XXFI_CURRENCIES_T XxfiCurrenciesEO WHERE XxfiCurrenciesEO.CURRENCY_CODE =ContractHeadersEO.CURRENCY_CODE and rownum=1) AS VIEW_ATTR, 
       (select OH.CUSTOMER_NAME from okc_k_headers_vl OH WHERE OH.CONTRACT_ID =XxboqCertifiedDetailsEO.CONTRACT_ID) AS VIEW_ATTR, 
