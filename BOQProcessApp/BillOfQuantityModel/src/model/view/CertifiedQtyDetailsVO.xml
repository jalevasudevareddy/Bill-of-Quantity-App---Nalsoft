       nvl((select sum(selling_amount) from xxboq_bill_of_qty_lines_t where price_code is not null and line_type = 'M' and estimated = 'N' and boq_header_id = XxboqCertifiedDetailsEO.BOQ_HEADER_ID),0) AS VIEW_ATTR, 
       nvl((select sum(selling_amount) from xxboq_bill_of_qty_lines_t where price_code is not null and line_type = 'V' and estimated = 'N' and boq_header_id = XxboqCertifiedDetailsEO.BOQ_HEADER_ID),0) AS VIEW_ATTR, 
       nvl((select sum(selling_amount) from xxboq_bill_of_qty_lines_t where price_code is not null and line_type = 'C' and estimated = 'N' and boq_header_id = XxboqCertifiedDetailsEO.BOQ_HEADER_ID),0) AS VIEW_ATTR, 
       (select case when at.Coun= 0 then 'None' when at.coun = 1 then (select atta.file_name from xx_attachments_t atta where atta.attachment_code='BOQ_Certification' and atta.reference_id = XxboqCertifiedDetailsEO.CERTIFIED_HEADER_ID) else 'Multiple' end from (select count(att.attachment_id) coun from xx_attachments_t att where att.attachment_code='BOQ_Certification' and att.reference_id = XxboqCertifiedDetailsEO.CERTIFIED_HEADER_ID) at) AS VIEW_ATTR, 
       (select ph.pc_status from xxboq_pc_hdr_t ph where ph.certified_header_id = XxboqCertifiedDetailsEO.CERTIFIED_HEADER_ID and ph.pc_status&lt;>'CL') AS VIEW_ATTR, 
       XxboqCertifiedDetailsEO.CONTRACT_ID, 
       ContractHeadersEO.CONTRACT_NUMBER, 
       ContractHeadersEO.CONTRACT_ID AS CONTRACT_ID1, 
       ContractHeadersEO.CURRENCY_CODE, 
       ContractHeadersEO.COGNOMEN, 
       XxboqCertifiedDetailsEO.VALUATION_DATE, 
       XxboqCertifiedDetailsEO.RETENTION_RELEASE, 
       (  (select  nvl(sum(nvl(il2.CERTIFIED_CUMMULATIVE_QTY,0)*il2.current_boq_rate),0) from XXBOQ_CERTIFIED_LINES_T il2 where il2.CERTIFIED_HEADER_ID = XxboqCertifiedDetailsEO.CERTIFIED_HEADER_ID )+ XxboqCertifiedDetailsEO.advance_received - XxboqCertifiedDetailsEO.advance_recovered - XxboqCertifiedDetailsEO.retention + XxboqCertifiedDetailsEO .retention_release - XxboqCertifiedDetailsEO.other_payments ) AS VIEW_ATTR"
  FromList="XXBOQ_CERTIFIED_DETAILS_T XxboqCertifiedDetailsEO, XXINV_BUSINESS_UNITS_T XxinvBusinessUnitsEO, XXPJ_PROJECTS_T XxpjProjectsEO, OKC_K_HEADERS_VL ContractHeadersEO"
