  <SQLQuery>
    <![CDATA[SELECT
     pc_header_id,
     bu_id,
     project_id,
     customer_id,
     contract_value,
     pc_date,
     document_type,
     pc_num,
     pc_document_num,
     revision,
     remarks,
     pc_status,
     period,
     pay_terms,
     prev_pc_document_num,
     prev_document_type,
     prev_period,
     ar_flag,
     gl_flag,
     reversel_flag,
     creation_date,
     created_by,
     last_updated_date,
     last_updated_by,
     pc_approved_date,
     pc_approved_by,
     certified_header_id,
     master_cert_header_id,
     bill_to_site_use_id,
     ship_to_site_use_id,
     master_pc_header_id,
     parent_pc_header_id,
     (
         SELECT
             pc_status
         FROM
             xxboq_pc_hdr_t
         WHERE
             pc_header_id = (
                 SELECT
                     MAX(h2.pc_header_id)
                 FROM
                     xxboq_pc_hdr_t h2
                 WHERE
                     h2.bu_id =:p_bu_id
                     AND h2.contract_id =:p_contract_id
             )
     ) max_status,
     (
         SELECT
             pc_document_num
         FROM
             xxboq_pc_hdr_t
         WHERE
             pc_header_id = (
                 SELECT
                     MAX(h2.pc_header_id)
                 FROM
                     xxboq_pc_hdr_t h2
                 WHERE
                     h2.bu_id =:p_bu_id
                     AND h2.contract_id =:p_contract_id
             )
     ) max_doc_num,
      (
         SELECT
             period
         FROM
             xxboq_pc_hdr_t
         WHERE
             pc_header_id = (
                 SELECT
                     MAX(h2.pc_header_id)
                 FROM
                     xxboq_pc_hdr_t h2
                 WHERE
                     h2.bu_id =:p_bu_id
                     AND h2.contract_id =:p_contract_id
             )
     ) max_period
 FROM
     xxboq_pc_hdr_t h1
 WHERE
     h1.pc_header_id = (
         SELECT
             MAX(h2.pc_header_id)
         FROM
             xxboq_pc_hdr_t h2
         WHERE
             h2.bu_id =:p_bu_id
             AND h2.contract_id =:p_contract_id
             AND h2.pc_status != 'CL'
     )]]>
  </SQLQuery>
